rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /tickets/{ticketId} {
      allow read: if request.auth.uid != null;
      allow write: if request.auth.uid != null;
    }
    match /ticketTags/{ticketTagId} {
      allow read: if request.auth.uid != null;
      allow write: if request.auth.uid != null;
    }
  }
}
